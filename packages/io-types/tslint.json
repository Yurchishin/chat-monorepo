{
  "extends": [
    "../../tslint.common.json"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/*",
      "dist/*",
      "src/**/*.spec.ts"
    ]
  },
  "rules": {
    "no-submodule-imports": [
      true,
      "validator",
      "fp-ts",
      "io-ts",
      "io-ts-types"
    ],
    "match-default-export-name": false,
    "strict-boolean-expressions": false,
    "no-unsafe-any": false
  }
}
